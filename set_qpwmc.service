[Unit]
Description=Run set_qpmcw_mode on startup
Before=voice_call_receive.service

# https://serverfault.com/questions/1053369/systemd-how-to-start-service-after-another-service-started

[Service]
Type=oneshot
ExecStartPre=/usr/bin/sleep 40
ExecStart=/usr/bin/bash -c 'sudo /home/judy/repos/remote-phone/set_qpwmc_mode.py'
# the log is at /home/judy/py_set_qpwmc.log
#StandardOutput=file:/home/judy/set_qpwmc.log
#StandardError=file:/home/judy/set_qpwmc_err.log
WantedBy=voice_call_receive.target

[Install]
WantedBy=default.target
